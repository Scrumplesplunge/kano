function fib(n : integer) : integer {
  var a : integer = 0;
  var b : integer = 1;
  # TODO: Implement comparison operators and replace this with `i < n`.
  var i : integer = n;
  while (i) {
    i = i - 1;
    b = a + b;
    a = b - a;
  }
  return a;
}

function main() : integer {
  return fib(10);
}
